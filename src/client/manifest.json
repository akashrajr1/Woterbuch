{
    "name": "Wöterbuch",
    "version": "1.0",
    "description": "German-English Dictionary",
    "manifest_version": 2,
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": "static/icon.png"
    },
    "icons": {
        "128": "static/icon.png"
    },
    "commands": {
        "_execute_browser_action": {
            "suggested_key": {
                "default": "Ctrl+D",
                "mac": "MacCtrl+D"
            },
            "description": "Opens Wöterbuch"
        }
    }
}